{{#if citation_key}}

<div class="bib_div">

<details data-key="{{citation_key}}" class=ref>

{{!-- citation --}}
<summary class="citation">
{{#if authors}}{{#each authors}}{{this}}{{#unless @last}}, {{/unless}}{{/each}} - {{/if}}{{#if title}}{{#if url}}<a href="{{url}}" target="_blank">{{/if}}{{title}}{{#if url}}</a>{{/if}} - {{/if}}{{#if pub_year}}{{pub_year}}.{{/if}}
</summary>

{{!-- summary/abstract --}}
{{#if summary}}
<section class=abstract>
<h1>Summary/Abstract</h1>
<div>{{summary}}</div>
</section>
{{/if}}

</details>

<br/>

</div>

{{/if}}